{
  "name": "mbund-resume",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "work": "concurrently \"npm run dev\" \"npm run watch-print\"",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "watch-print": "onchange 'src/**/*.astro' 'resume.json' -- chromium --headless --disable-gpu --print-to-pdf=resume.pdf --no-margins http://localhost:4321 2> /dev/null"
  },
  "dependencies": {
    "@astrojs/tailwind": "^5.1.0",
    "astro": "^4.1.2",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "onchange": "^7.1.0",
    "prettier": "^3.2.1",
    "prettier-plugin-astro": "^0.12.3",
    "prettier-plugin-tailwindcss": "^0.5.11"
  }
}
